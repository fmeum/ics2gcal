{
  "manifest_version": 3,
  "name": "ICS to GCal",
  "short_name": "ICS2GCal",
  "version": "1.0.0",
  "description": "Import .ics files into Google Calendar with just two clicks",
  "icons": {
    "128": "images/logo_active.png"
  },
  "author": "Fabian Henneke, Austin Clerkin, Marc Bernard",
  "homepage_url": "https://github.com/FabianHenneke/ics2gcal",
  "key": "",
  "action": {
    "default_title": "Authorize ICS to GCal to access (read/write) your Google Calendar",
    "default_icon": {
      "128": "images/logo_inactive.png"
    }
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "identity",
    "scripting"
  ],
  "host_permissions": [
    "https://www.googleapis.com/calendar/v3/*",
    "<all_urls>"
  ],
  "oauth2": {
    "client_id": "284525147469-lns92r5f57ppmd7ht8m311tuc7jsog2s.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.readonly",
      "https://www.googleapis.com/auth/calendar.events",
      "https://www.googleapis.com/auth/calendar"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "snackbar.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/*",
        "images/*",
        "snackbar.js",
        "snackbar.css"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ]
}
